ex:SizeConstraint a sh:NodeShape ;
    sh:property [ sh:in ( ex:small ex:medium ex:large ) ;
            sh:path ex:hasSize ] ;
    sh:targetClass ex:Game .

ex:UniqueIDConstraint a sh:PropertyShape ;
    sh:datatype xsd:int ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:path ex:hasID ;
    sh:unique true .

ex:UniqueURLConstraint a sh:PropertyShape ;
    sh:maxCount 1 ;
    sh:message "Value must be a valid URL." ;
    sh:minCount 1 ;
    sh:path ex:hasURL ;
    sh:pattern "^(https?://[a-zA-Z0-9.-]+\\.[a-zA-Z]{2,})?$" ;
    sh:unique true .

